<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="creditangelteam.myapplication.WishListPage$PlaceholderFragment">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="1"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_weight="0.02"
            android:layout_height="105dp">
            <LinearLayout
                    android:layout_marginLeft="16px"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                android:weightSum="1">
                    <com.github.mikephil.charting.charts.PieChart
                        android:id="@+id/chart1"
                        android:layout_width="match_parent"
                        android:layout_weight="1.58"
                        android:layout_height="95dp" />

                <TextView
                    android:layout_width="match_parent"
                    android:id="@+id/WilliamTxt"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="William Haung"
                    android:gravity="center"
                    android:layout_height="20dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="16px"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:weightSum="1">

                <com.github.mikephil.charting.charts.PieChart
                    android:id="@+id/chart2"
                    android:layout_width="match_parent"
                    android:layout_weight="1.36"
                    android:layout_height="95dp" />

                <TextView
                    android:text="Emily Zeng"
                    android:layout_width="match_parent"
                    android:id="@+id/EmilyTxt"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:gravity="center"
                    android:layout_height="20dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="115dp">

            <LinearLayout
                android:layout_marginLeft="16px"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <com.github.mikephil.charting.charts.PieChart
                    android:id="@+id/chart3"
                    android:layout_width="match_parent"
                    android:layout_height="95dp" />

                <TextView
                    android:text="Yao"
                    android:layout_width="match_parent"
                    android:id="@+id/YaoTxt"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:gravity="center"
                    android:layout_height="20dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="16px"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <com.github.mikephil.charting.charts.PieChart
                    android:id="@+id/chart4"
                    android:layout_width="match_parent"
                    android:layout_height="95dp" />

                <TextView
                    android:text="Sean Wu"
                    android:layout_width="match_parent"
                    android:id="@+id/SeanTxt"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:gravity="center"
                    android:layout_height="20dp" />
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="1"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_weight="0.02"
            android:layout_height="105dp">
            <LinearLayout
                android:layout_marginLeft="16px"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:weightSum="1">
                <com.github.mikephil.charting.charts.PieChart
                    android:id="@+id/chart5"
                    android:layout_width="match_parent"
                    android:layout_weight="1.58"
                    android:layout_height="95dp" />

                <TextView
                    android:layout_width="match_parent"
                    android:id="@+id/WilliamTxt2"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="William Haung"
                    android:gravity="center"
                    android:layout_height="20dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="16px"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:weightSum="1">

                <com.github.mikephil.charting.charts.PieChart
                    android:id="@+id/chart6"
                    android:layout_width="match_parent"
                    android:layout_weight="1.36"
                    android:layout_height="95dp" />

                <TextView
                    android:text="Emily Zeng"
                    android:layout_width="match_parent"
                    android:id="@+id/EmilyTxt2"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:gravity="center"
                    android:layout_height="20dp" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="115dp">

            <LinearLayout
                android:layout_marginLeft="16px"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <com.github.mikephil.charting.charts.PieChart
                    android:id="@+id/chart7"
                    android:layout_width="match_parent"
                    android:layout_height="95dp" />

                <TextView
                    android:text="Yao"
                    android:layout_width="match_parent"
                    android:id="@+id/YaoTxt2"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:gravity="center"
                    android:layout_height="20dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="16px"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <com.github.mikephil.charting.charts.PieChart
                    android:id="@+id/chart8"
                    android:layout_width="match_parent"
                    android:layout_height="95dp" />

                <TextView
                    android:text="Sean Wu"
                    android:layout_width="match_parent"
                    android:id="@+id/SeanTxt2"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:gravity="center"
                    android:layout_height="20dp" />
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="230dp">
    <com.github.mikephil.charting.charts.PieChart
        android:id="@+id/chart9"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    </LinearLayout>

</RelativeLayout>
